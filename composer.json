{
    "name": "rockhopsoft/survlooporg",
    "description": "Survloop is a Laravel-based engine for designing a database and creating a mobile-friendly user interface to fill it.",
    "keywords": [
    	"laravel", 
    	"Survloop", 
    	"survey", 
    	"database designer", 
    	"user experience", 
    	"forms", 
    	"mobile friendly", 
    	"mobile first", 
    	"branching tree",
    	"nodes"
    ],
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Morgan Lesko",
            "homepage": "https://github.com/rockhopsoft/survlooporg",
            "email": "rockhoppers@runbox.com",
            "role": "Developer"
        }
    ],
    "require": {
        "rockhopsoft/survloop": "^0.2"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "SurvloopOrg\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RockHopSoft\\SurvloopOrg\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "RockHopSoft\\SurvloopOrg\\SurvloopOrgServiceProvider"
            ],
            "aliases": {
                "SurvloopOrg": "RockHopSoft\\SurvloopOrg\\SurvloopOrgFacade"
            }
        }
    }
}
